<?xml version="1.0" encoding="utf-8"?>
<!-- TopBar 导航栏那的背景色选择器-->
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:drawable="@color/red_btn_color_normal" />
    <item android:state_focused="true" android:drawable="@color/red_btn_color_normal" />
    <item android:state_focused="false" android:state_pressed="true" android:drawable="@color/red_btn_color_normal" />
    <item android:drawable="@android:color/transparent" />
</selector>