<?xml version="1.0" encoding="utf-8"?>
<!--文本颜色选择器  -->
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@android:color/darker_gray" android:state_enabled="false"/>
    <item android:color="@android:color/darker_gray" android:state_selected="true"/>
    <item android:color="@android:color/darker_gray" android:state_pressed="true"/>
    <item  android:color="@android:color/white" />
</selector>
