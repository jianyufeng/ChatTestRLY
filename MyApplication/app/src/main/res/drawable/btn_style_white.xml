<?xml version="1.0" encoding="utf-8"?>
<!-- 创建群组的选择群组图像的选择器 -->
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/android_width_8" />
            <solid android:color="@color/select_pic_default" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/android_width_8" />
            <solid android:color="@color/select_pic_press" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/android_width_8" />
            <solid android:color="@color/select_pic_default" />
        </shape>
    </item>
</selector>